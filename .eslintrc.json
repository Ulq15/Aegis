{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", {
            "ignoreStrings":true,
            "ignoreTemplateLiterals":true
        }],
        "require-jsdoc": "off",
        "semi": ["off", "never"],
        "quotes": ["off", "double"],
        "indent": ["error", 2],
        "comma-dangle": ["error", "never"],
        "object-curly-spacing": ["on"]
    }
}
